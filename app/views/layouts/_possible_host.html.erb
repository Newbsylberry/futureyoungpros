    <div class="well">
      <h3>Possible Hosts:</h3>
      <div class="table-responsive">
	<table class="table table-hover">
	  <thead>
	    <tr>
	      <th>First Name:</th>
	      <th>Last Name:</th>
	      <th>Email Address:</th>
	      <th>Business Name:</th>
	    </tr>
	  </thead>
	  <tbody>
	    <% @mous.each do |mou| %>
	    <tr>
	      <td><%= mou.firstName %></td>
	      <td><%= mou.lastName %></td>
	      <td><%= mou.emailAddress %></td>
	      <td><%= mou.business.name if mou.business %></td>
	    </tr>
	    <% end %>
	</table>
      </div>
    </div>
