<div class="well">
<h1>Job Shadow  Hosts</h1>
</div>
<div class="row">
  <%= render 'layouts/admin_menu' %>
  <div class="col-sm-9 col-md-9 col-lg-9">
    <div class="table-responsive">
      <table class="table table-hover">
	<thead>
	  <tr>
	    <th>Email Address</th>
	    <th>Category</th>
	    <th>Business Name</th>
	    <th>Job Title</th>
	    <th>Considerations</th>
	    <th></th>
	    <th></th>
	    <th></th>
	  </tr>
	</thead>
	
	<tbody>
	  <% @mous.each do |mou| %>
	  <tr>
            <td><%= mou.emailAddress %></td>
            <td><%= mou.category.name if mou.category %></td>
	    <td><%= mou.business.name if mou.business %></td>
	    <td><%= mou.jobTitle %></td>
            <td><%= mou.considerations %></td>
            <td><%= link_to 'Show', mou %></td>
            <td><%= link_to 'Edit', edit_mou_path(mou) %></td>
            <td><%= link_to 'Destroy', mou, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	  </tr>
	  <% end %>
	</tbody>
      </table>
    </div>
    <br>
    <%= link_to 'New Mou', new_mou_path %>
    <%= link_to 'Home Page', welcome_admin_path %>
  </div>
</div>




