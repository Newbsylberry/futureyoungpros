<% if flash[:notice] %>
  <p id="notice"><%= notice %></p>
<% end %>
<div class="row">
  <div class="col-xs-6 col-sm-4 col-md-4 col-lg-4">
    <p>
      <strong>Student First Name:</strong>
      <%= @student_interest.firstName %>
    </p>
    <p>
      <strong>Student Last Name:</strong>
      <%= @student_interest.lastName %>
    </p>
    <p>
      <strong>School:</strong>
      <%= @student_interest.school.name if @student_interest.school %>
    </p>
    <p>
      <Strong>School Point of Contact:</strong>
      <%= @student_interest.school.contactFirstName + " " +  @student_interest.school.contactLastName if @student_interest.school %>
    <p>
    <p>
      <Strong>Point of Contact Email:</strong>
      <%= @student_interest.school.contactEmail if @student_interest.school %>
    <p>
      <strong>Category:</strong>
      <%= @student_interest.category_id %>
    </p>
  </div>
  <div class="col-xs-7 col-sm-7 col-md-7 col-lg-7">
    <%= render 'layouts/match_form' %>
  </div>
</div>
  <%= link_to 'Edit', edit_student_interest_path(@student_interest) %> |
  <%= link_to 'Back', student_interests_path %>
